// Place your settings in this file to overwrite default and user settings.
{
  "javascript.validate.enable": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/elm-stuff": true,
    "**/dist": true
  },
  "prettier.disableLanguages": ["javascript", "jsx"],
  "prettier.ignorePath": ".*",
  "editor.tabSize": 2,
  "editor.formatOnPaste": false,
  "cSpell.words": [
    "ecpa",
    "webpush"
  ],
}