{
  "name": "ss-omnidashboard-ui",
  "version": "1.1.0",
  "description": "Get started with ES2015, React and Redux. Including Webpack, ESLint, react-router, fetch ...",
  "scripts": {
    "postinstall": "bower install && pulp build && npm run build",
    "preinstall": "yarn global add purescript pulp bower",
    "start": "./node_modules/.bin/babel-node -- server/index.js",
    "server": "nodemon --exec ./node_modules/.bin/babel-node -- server/index.js",
    "dev": "npm run server & webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "build-purescript": "rm -rf output && pulp build -I server && rm -rf dist ",
    "build": "yarn build-purescript && webpack --config webpack/webpack.config.prod.js  && cp -a assets/* dist/",
    "prod": "cd dist && python -m SimpleHTTPServer 8080"
  },
